{
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "SAMEORIGIN",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Permissions-Policy": "geolocation=(), microphone=(), camera=()",
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' bat.bing.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; connect-src 'self' bat.bing.com; frame-ancestors 'self'"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".woff2": "font/woff2",
    ".woff": "font/woff"
  },
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/Projects/*", "/legacy/*", "/*.json"]
  }
}
